## 目录
CREATE TABLE `test`.`test_menu` (
  `id` INT NOT NULL COMMENT '自身id',
  `text` VARCHAR(45) NOT NULL COMMENT '标题',
  `icon` VARCHAR(45) NULL COMMENT '图标',
  `is_header` TINYINT NULL,
  `url` VARCHAR(45) NULL COMMENT '跳转链接',
  `target_type` VARCHAR(14) NULL COMMENT 'blank 代表打开新页面  ajax 代表ajax方式打开页面 iframe-tab 代表单iframe页面',
  `is_open` TINYINT NULL COMMENT '是否打开',
  `parent_id` INT NULL COMMENT '父id',
  `before_id` INT NULL COMMENT '位置相连的前一节点id，插入删除时需要相连的节点的前节点id',
  `status` SMALLINT NULL COMMENT '状态：0可用，1不可用',
  PRIMARY KEY (`id`))
COMMENT = '目录';